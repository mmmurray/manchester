{
  "name": "manchester",
  "version": "0.0.0-semantically-released",
  "description": "Accessible React component library.",
  "author": "Mark Murray",
  "license": "MIT",
  "repository": "git@github.com:mmmurray/manchester.git",
  "homepage": "https://mark.murray.xyz/manchester/",
  "keywords": [
    "accessible",
    "react",
    "component",
    "library",
    "typescript"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "mmm build",
    "build:storybook": "rm -rf ./dist/storybook && build-storybook -o ./dist/storybook",
    "build:styleguide": "rm -rf ./dist/styleguide && styleguidist build",
    "commit": "mmm commit",
    "jest": "mmm jest",
    "release": "mmm release",
    "storybook": "start-storybook -p 6006",
    "styleguide": "styleguidist server",
    "test": "mmm test",
    "test:coverage": "mmm test:coverage",
    "test:lint": "mmm test:lint"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-pixel": "^0.3.3",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.1",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "babel-loader": "^8.0.5",
    "jest-component-snapshot": "^0.1.4",
    "mmm-scripts": "^0.6.1",
    "puppeteer": "^1.18.0",
    "react-docgen-typescript": "^1.12.5",
    "react-styleguidist": "^9.1.9"
  },
  "resolutions": {
    "react-styleguidist/@vxna/mini-html-webpack-template": "1.0.0",
    "react-styleguidist/mini-html-webpack-plugin": "1.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-mmm/ts-react"
  },
  "husky": {
    "hooks": {
      "commit-msg": "mmm precommit"
    }
  },
  "prettier": "mmm-scripts/prettier.config"
}
